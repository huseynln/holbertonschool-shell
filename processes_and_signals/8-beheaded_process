#!/usr/bin/env bash
# Find the Process ID (PID) of the process named "7-highlander"
# The 'pgrep' command searches for processes based on name and prints their PIDs.
pkill -9 7-highlander

# Check the exit status of pkill
if [ $? -eq 0 ]; then
    echo "Process 7-highlander successfully killed."
else
    # An exit status of 1 (or other non-zero) typically means no process was found 
    # OR the kill failed due to permissions. In this environment, it's safer to assume 
    # the process was simply not found when the script ran.
    echo "Attempted to kill process 7-highlander."
fi
